 <%- include("head") %>
  <body class="container">
    <!-- Header section -->
    <%- include("header") %>
   
<main class="modify">
  <h2 class="modify__heading">Modify or Delete</h2>
  
<%for (let department of departments){%>

  <div class="modify__content">
    <h2 class="modify__content--title"><%= department.name%></h2>
    <input type="text" class="Id" value="<%= department._id%>" hidden>
    <a class="modify__content--link" href="/editDepartments/<%=department._id%>">Edit</a>
   <a class="modify__content--link delete" href="#">Delete</a>

  </div>
  <%}%>
</main>

    <%- include("footer") %>
  </body>
</html>