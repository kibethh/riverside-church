 <%- include("head") %>
  <body class="container">
    <!-- Header section -->
    <%- include("header") %>
<main class="modify">
  <h2 class="modify__heading">Modify or Delete</h2>
  <% for(let member of members){%>
  <div class="modify__content">
    <h2 class="modify__content--title"><%=member.member_name%></h2>
    <input type="text" class="Id" value="<%= member._id%>" hidden>
    <h2 class="modify__content--title"><%=member.createdAt%></h2>
    <a class="modify__content--link" href="/editMembers/<%= member._id%>">Edit</a>
   <a class="modify__content--link delete" href="#">Delete</a>

    
  </div>
  <%}%>
</main>

    <%- include("footer") %>
  </body>
</html>