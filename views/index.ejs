            <div class="slider__img-container--list__item--content">
              <h2 class="slider__img-container--list__item--content__title">Heading</h2>
              <p class="slider__img-container--list__item--content__para">Paragraph content</p>
            </div>
          </li>
          <li class="slider__img-container--list__item">
            <img src="/img/Gallery/<%= images[4]%>" alt="villa" />
            <div class="slider__img-container--list__item--content">
              <h2 class="slider__img-container--list__item--content__title">Heading</h2>
              <p class="slider__img-container--list__item--content__para">Paragraph content</p>
            </div>
          </li>
          <li class="slider__img-container--list__item">
            <img src="/img/Gallery/<%= images[5]%>" alt="villa" />
            <div class="slider__img-container--list__item--content">
              <h2 class="slider__img-container--list__item--content__title">Heading</h2>
