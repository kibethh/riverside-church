            <div class="slider__img-container--list__item--content">
              <h2 class="slider__img-container--list__item--content__title">Heading</h2>
              <p class="slider__img-container--list__item--content__para">Paragraph content</p>
            </div>
          </li>
        </ul>
        <!-- Button Left -->
      <button class="slider__btn slider__btn--left">
        <i class="icons fas fa-caret-left"></i>
      </button>
        <!-- Button Right -->
        <button class="slider__btn slider__btn--right">
          <i class="icons fas fa-caret-right"></i>
        </button>
       
      </div>
    </section>

    <!-- Coordinators section -->
    
    <!-- Testimonials section -->
    <section class="testimonials">
      <h2 class="testimonials__title">Members</h2>
      <div class="testimonials__content">
        <!-- Sliders -->
        <div class="testimonials__content--slider">
          <!-- Slide 1 -->
          <div class="testimonials__content--slider__slide active">
            <p class="testimonials__content--slider__slide--para">
              
              <%= dispMembers[0].member_opinion %> 
            </p>
            <h3 class="testimonials__content--slider__slide--title">
              
             <%= dispMembers[0].member_name %> 
            </h3>
          </div>
          <!-- Slide 2 -->
          <div class="testimonials__content--slider__slide">
            <p class="testimonials__content--slider__slide--para">
              
              <%= dispMembers[1].member_opinion %>
            </p>
            <h3 class="testimonials__content--slider__slide--title">
              <%= dispMembers[1].member_name %>
            </h3>
          </div>
          <!-- Slide 3 --> -->
          <div class="testimonials__content--slider__slide"> 
            <p class="testimonials__content--slider__slide--para">
              
              <%= dispMembers[2].member_opinion %>
            </p>
            <h3 class="testimonials__content--slider__slide--title">
              
              <%= dispMembers[2].member_name %>
            </h3>
          </div> 
        </div>

        <!-- Indicator -->
        <div class="testimonials__content--indicator"> 
          <img
            src="/img/members/<%= dispMembers[0].photo %>"
            alt="member-1"
            class="testimonials__content--indicator__img active"
            data-id="0"
          /><img
            src="/img/members/<%= dispMembers[1].photo %>"
            alt="member-2"
            class="testimonials__content--indicator__img"
            data-id="1"
          /><img
            src="/img/members/<%= dispMembers[2].photo %>"
            alt="member-3"
            class="testimonials__content--indicator__img"
            data-id="2"
          />
        </div>
      </div>
     
    </section>
    
    <!-- Scroll to top button -->
    <div class="scroll"></div>
   
    <%- include("footer") %>
  </body>
</html>
